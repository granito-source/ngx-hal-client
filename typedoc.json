{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Angular HAL Client",
  "includeVersion": true,
  "entryPoints": [
    "./src/public-api.ts"
  ],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "skipErrorChecking": true,
  "visibilityFilters": {
    "inherited": true,
    "external": true
  },
  "externalSymbolLinkMappings": {
    "typescript": {
      "Array": "#",
      "Object": "#"
    },
    "rxjs": {
      "Observable": "#"
    },
    "@angular/common": {
      "HttpClient": "#"
    }
  },
  "tsconfig": "./tsconfig.lib.json",
  "out": "build/typedoc"
}
